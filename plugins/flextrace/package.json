{
  "name": "flextrace-opencode",
  "version": "2.0.2",
  "description": "Task/event tracing plugin for OpenCode (agent/skill/tool tasks, event/export/analyze).",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    }
  },
  "bin": {
    "tracectl": "dist/src/cli.js"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "dist",
    "README.md",
    "docs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run typecheck && npm run build",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "pack:check": "npm pack --dry-run"
  },
  "keywords": [
    "opencode",
    "trace",
    "tracepoint",
    "plugin",
    "observability"
  ],
  "author": "jiqi",
  "devDependencies": {
    "@types/node": "^22.13.10",
    "typescript": "^5.8.2"
  }
}
